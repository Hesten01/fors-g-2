#include <stdio.h>

int controle(int weight, int motor) {
  if (weight / motor <= 5.6)
    return 1;
}
int main() {
  int weight;
  float motor;

  printf("hvor mange motoerer er der?");
  scanf("%d", &weight);
  printf("hvor meget vÃ¦gt er der?");
  scanf("%f", &motor);
  if (controle(weight, motor))
    printf("ja den kan godt holden\n");
  else
    printf("nej den kan ikke holde\n");
}